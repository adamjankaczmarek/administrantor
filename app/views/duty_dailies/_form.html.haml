= form_for(@duty_daily) do |f|
  - if @duty_daily.errors.any?
    #error_explanation
      %h2
        = pluralize(@duty_daily.errors.count, "error")
        prohibited this duty_daily from being saved:
      %ul
        - @duty_daily.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :duty_daily_schema
    %br/
    = f.text_field :duty_daily_schema
  .field
    = f.label :service_daily
    %br/
    = f.text_field :service_daily
  .actions
    = f.submit
