= form_for(@altar_boy_duty) do |f|
  - if @altar_boy_duty.errors.any?
    #error_explanation
      %h2
        = pluralize(@altar_boy_duty.errors.count, "error")
        prohibited this altar_boy_duty from being saved:
      %ul
        - @altar_boy_duty.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :altar_boy
    %br/
    = f.text_field :altar_boy
  .field
    = f.label :duty
    %br/
    = f.text_field :duty
  .actions
    = f.submit
